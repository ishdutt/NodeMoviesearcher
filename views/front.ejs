<!DOCTYPE html>
<html>
<head>
	<title>Movies IC</title>
	 <script type="text/javascript" src="particle.js"></script>
	 <link rel="stylesheet" type="text/css" href="index.css">
   <link href="https://fonts.googleapis.com/css?family=Arapey|Kaushan+Script" rel="stylesheet">
   <link rel="icon" type="Icon (.ico)" href="https://i2.wp.com/darnleyroadstudios.com/wp-content/uploads/sites/3/2017/02/Whitelabelfilms_favicon_512square-1.png?w=1080"
</head>

<body id="particles-js">
  

  <div id="particles-js" class='container'>
    <h1>Movies information center</h1>
    <form action="/result" method="GET" >
      <input type="text" name="movieName" placeholder="Movie Name">
      <button>here you go Cinephiles</button>
    </form>
  </div>






<script type="text/javascript">
 particlesJS("particles-js",{
  "particles": {
    "number": {
      "value": 190,
      "density": {
        "enable": true,
        "value_area": 475
      }
    },
    "color": {
      "value": "#ffffff"
    },
    "shape": {
      "type": "edge",
      "stroke": {
        "width": 0,
        "color": "#000000"
      },
      "polygon": {
        "nb_sides": 5
      },
      "image": {
        "src": "img/github.svg",
        "width": 100,
        "height": 100
      }
    },
    "opacity": {
      "value": 0.5,
      "random": false,
      "anim": {
        "enable": false,
        "speed": 1,
        "opacity_min": 0.1,
        "sync": false
      }
    },
    "size": {
      "value": 3,
      "random": true,
      "anim": {
        "enable": false,
        "speed": 40,
        "size_min": 0.1,
        "sync": false
      }
    },
    "line_linked": {
      "enable": true,
      "distance": 144,
      "color": "#ffffff",
      "opacity": 0.4,
      "width": 1
    },
    "move": {
      "enable": true,
      "speed": 4,
      "direction": "none",
      "random": false,
      "straight": false,
      "out_mode": "out",
      "bounce": false,
      "attract": {
        "enable": false,
        "rotateX": 600,
        "rotateY": 1200
      }
    }
  },
  "interactivity": {
    "detect_on": "canvas",
    "events": {
      "onhover": {
        "enable": true,
        "mode": "repulse"
      },
      "onclick": {
        "enable": true,
        "mode": "push"
      },
      "resize": true
    },
    "modes": {
      "grab": {
        "distance": 400,
        "line_linked": {
          "opacity": 1
        }
      },
      "bubble": {
        "distance": 400,
        "size": 40,
        "duration": 2,
        "opacity": 8,
        "speed": 3
      },
      "repulse": {
        "distance": 200,
        "duration": 0.4
      },
      "push": {
        "particles_nb": 4
      },
      "remove": {
        "particles_nb": 2
      }
    }
  },
  "retina_detect": true
});
</script>
</body>
</html>
